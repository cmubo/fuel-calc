{
    "expo": {
        "name": "fuel-calc",
        "slug": "fuel-calc",
        "version": "1.0.0",
        "platforms": ["ios", "android"],
        "orientation": "portrait",
        "icon": "./assets/images/ios/ios-light.png",
        "scheme": "fuelcalc",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.anonymous.fuel-calc",
            "icon": {
                "dark": "./assets/images/ios/ios-dark.png",
                "light": "./assets/images/ios/ios-light.png",
                "tinted": "./assets/images/ios/ios-tinted.png"
            }
        },
        "android": {
            "icon": "./assets/images/android/adaptive-icon.png",
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/android/adaptive-icon.png",
                "backgroundColor": "#020617"
            },
            "edgeToEdgeEnabled": true,
            "package": "com.anonymous.fuelcalc"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "react-native-edge-to-edge",
                {
                    "android": {
                        "parentTheme": "Default",
                        "enforceNavigationBarContrast": false
                    }
                }
            ],
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/splash-icon-dark.png",
                    "resizeMode": "contain",
                    "backgroundColor": "#94a3b8",
                    "dark": {
                        "image": "./assets/images/splash-icon-light.png",
                        "backgroundColor": "#020617",
                        "resizeMode": "contain"
                    },
                    "imageWidth": 200
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}
